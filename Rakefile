begin
  require 'spec/rake/spectask'
  desc 'Run specs'
  Spec::Rake::SpecTask.new('spec') do |t|
    t.spec_files = FileList['spec/**/*_spec.rb']
  end
rescue LoadError; end

desc 'Build the gem'
task :gem do
  `gem build oneliner.gemspec`
  `mkdir -p pkg && mv oneliner-*.gem pkg 2> /dev/null`
  puts <<-ARNOLD
